  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json --silent && copyfiles --error -f package*.json dist/ && copyfiles --error -f src/presentation/emails/*.hbs dist/src/presentation/emails/",
    "build:http": "yarn run build:http:dashboard && yarn run build:http:portfolio",
    "build:http:dashboard": "yarn tsed run generate-http-client --output ../dashboard/generated",
    "build:http:portfolio": "yarn tsed run generate-http-client --output ../portfolio/generated",
    "barrels": "barrelsby -c .barrelsby.json",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "format": "prettier --write ./**/*.{ts,tsx,scss,css,json}",
    "tsc": "tsc --pretty --noEmit",
    "test": "jest",
    "clean": "rm -rf dist && rm -rf .turbo && rm -rf logs && rm -rf node_modules && rm -f tsconfig.tsbuildinfo",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format"
  },
